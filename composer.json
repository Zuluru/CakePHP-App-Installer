    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Installer\\": "src"
        }
    },    
    "authors": [
        {
            "name": "Anuj Sharma",
            "email": "anujsharma9196@gmail.com"
        }
    ],
